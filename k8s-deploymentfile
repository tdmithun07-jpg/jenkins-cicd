apiVersion: apps/v1
kind: Deployment
metadata:
  name: jenkins-demo-app
spec:
  replicas: 2
  selector:
    matchLabels:
      app: jenkins-demo
  template:
    metadata:
      labels:
        app: jenkins-demo
    spec:
      containers:
        - name: app
          image: DOCKERHUB_USERNAME/jenkins-demo:latest
          ports:
            - containerPort: 3000
